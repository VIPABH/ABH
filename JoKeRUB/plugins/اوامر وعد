from telethon 
@l313l.ar_cmd(
    pattern="Clerk$",
    command=("Clerk", plugin_category),
    info={
        "᯽︙ الأسـتخدام": "إرسال معنى كلمة Clerk",
        "᯽︙ الشـرح": "يستجيب بإرسال معنى الكلمة 'Clerk'",
        "᯽︙ الأمـر": ["{tr}Clerk"],
    },
    groups_only=True,
    require_admin=False,
)
async def clerk_meaning(event):
    "Sends the meaning of the word 'Clerk'."
    if event.sender_id == 6806412130:
        await event.reply("كاتب")

